/**app.wxss**/
page{
  background-color: #F5F5F5;
  height: 100%;
  box-sizing: border-box;
  position: relative;
  letter-spacing:  2rpx;
  line-height: 1.5;
  font-size: 28rpx;
  font-family: PingFangSC-Regular,Helvetica Neue,Helvetica,Lantinghei SC,SimSun,sans-serif;
  -webkit-font-smoothing: antialiased;
}
view{
  padding: 0;
  margin: 0;
}
text{
  padding: 0;
  margin: 0;
}
text.em{
  font-weight: bold; 
}
.button{
  color: #fff;
  background-color: #F5A623;
  height: 88rpx;
  line-height: 88rpx;
  margin-top: 20rpx;
  border-radius: 6rpx;
  font-size: 32rpx;
  text-align: center;
  border: none;
}
.button-hover{
  background-color: #CC8003;
}
